{
    "family": "slim-taskdef",
    "executionRoleArn": "arn:aws:iam::255945442255:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "slim-taskdef",
            "image": "255945442255.dkr.ecr.ap-southeast-1.amazonaws.com/slim-nodejs:latest",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}